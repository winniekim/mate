<p id="notice"><%= notice %></p>

<h1>Photos</h1>

<table>
  <thead>
    <tr>
      <th>제목</th>
      <th>내용</th>
      <th>사진</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @photos.each do |photo| %>
      <tr>
        <td><%= photo.title %></td>
        <td><%= photo.content %></td>
        <td><%= image_tag photo.image.thumb.url %></td>
        <td><%= link_to '크게보기', photo %></td>
        <td><%= link_to '수정하기', edit_photo_path(photo) %></td>
        <td><%= link_to '삭제하기', photo, method: :delete, data: { confirm: '정말 삭제하시겠습니까?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to '새글작성', new_photo_path %>